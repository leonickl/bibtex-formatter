{
    "name": "leonickl/bibtex-formatter",
    "description": "A simplistic BibTeX formatter in PHP",
    "type": "projecct",
    "authors": [
        {
            "name": "Leo Nickl",
            "email": "leol@leonickl.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.4",
        "ext-pdo": "*",
        "leonickl/pxp-core": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.24",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "routes.php"
        ]
    },
    "bin": [
        "format-bibtex"
    ]
}